                "discovery_rules": [
                    {
                        "uuid": "5e6cad6f360747d990f7980ac4ec8fa2",
                        "name": "Asterisk Endpoints discovery",
                        "type": "DEPENDENT",
                        "key": "asterisk.endpoint.discovery",
                        "delay": "0",
                        "filter": {
                            "conditions": [
                                {
                                    "macro": "{#RESOURCE}",
                                    "value": "({$ARI.REGEX.TRUNK})",
                                    "formulaid": "A"
                                },
                                {
                                    "macro": "{#TECHNOLOGY}",
                                    "value": "({$ARI.REGEX.TRUNK.TECH})",
                                    "formulaid": "B"
                                }
                            ]
                        },
                        "lifetime_type": "DELETE_IMMEDIATELY",
                        "lifetime": "0",
                        "item_prototypes": [
                            {
                                "uuid": "11478118e96f4885bb8b6b36f6adf603",
                                "name": "Endpoint {#TECHNOLOGY}[{#RESOURCE}] count channels",
                                "type": "DEPENDENT",
                                "key": "asterisk.endpoints[{#TECHNOLOGY},{#RESOURCE},channels]",
                                "delay": "0",
                                "units": "ch",
                                "preprocessing": [
                                    {
                                        "type": "JSONPATH",
                                        "parameters": [
                                            "$.[?((@.resource=='{#RESOURCE}') && (@.technology=='{#TECHNOLOGY}'))].channel_ids.first().length()"
                                        ],
                                        "error_handler": "CUSTOM_VALUE",
                                        "error_handler_params": "0"
                                    }
                                ],
                                "master_item": {
                                    "key": "asterisk.endpoints.get"
                                }
                            },
                            {
                                "uuid": "e45c0d85e9af421f874f61778b7c8dc5",
                                "name": "Endpoint {#TECHNOLOGY}[{#RESOURCE}] State",
                                "type": "DEPENDENT",
                                "key": "asterisk.endpoints[{#TECHNOLOGY},{#RESOURCE},state]",
                                "delay": "0",
                                "value_type": "TEXT",
                                "trends": "0",
                                "preprocessing": [
                                    {
                                        "type": "JSONPATH",
                                        "parameters": [
                                            "$.[?((@.resource=='{#RESOURCE}') && (@.technology=='{#TECHNOLOGY}'))].state.first()"
                                        ],
                                        "error_handler": "CUSTOM_ERROR",
                                        "error_handler_params": "ERROR PROTOTYPE STATE"
                                    }
                                ],
                                "master_item": {
                                    "key": "asterisk.endpoints.get"
                                },
                                "tags": [
                                    {
                                        "tag": "endpoint",
                                        "value": "{#TECHNOLOGY}[{#RESOURCE}]"
                                    },
                                    {
                                        "tag": "state",
                                        "value": "{#STATE}"
                                    }
                                ],
                                "trigger_prototypes": [
                                    {
                                        "uuid": "678d55fba6da41b98586437b727d7570",
                                        "expression": "last(/AsteriskARI/asterisk.endpoints[{#TECHNOLOGY},{#RESOURCE},state],#4)<>\"online\"",
                                        "name": "Endpoint {#TECHNOLOGY}[{#RESOURCE}]",
                                        "priority": "HIGH",
                                        "manual_close": "YES",
                                        "dependencies": [
                                            {
                                                "name": "Asterisk ARI (RESTful API)",
                                                "expression": "last(/AsteriskARI/asterisk.ping,#2)<>\"pong\" or nodata(/AsteriskARI/asterisk.ping,90)=1"
                                            }
                                        ],
                                        "tags": [
                                            {
                                                "tag": "endpoint",
                                                "value": "{#TECHNOLOGY}[{#RESOURCE}]"
                                            },
                                            {
                                                "tag": "state",
                                                "value": "{#STATE}"
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "uuid": "5e2a87d3d2be4fe58f6da44791fc31d9",
                                "name": "Endpoints {#TECHNOLOGY} Count",
                                "type": "DEPENDENT",
                                "key": "asterisk.endpoints[{#TECHNOLOGY}]",
                                "delay": "0",
                                "trends": "0",
                                "preprocessing": [
                                    {
                                        "type": "JSONPATH",
                                        "parameters": [
                                            "$.[?(@.technology=='{#TECHNOLOGY}')].length()"
                                        ],
                                        "error_handler": "CUSTOM_ERROR",
                                        "error_handler_params": "ERROR COUNT TECHNOLOGY"
                                    }
                                ],
                                "master_item": {
                                    "key": "asterisk.endpoints.get"
                                }
                            }
                        ],
                        "graph_prototypes": [
                            {
                                "uuid": "74a9259263944e54a0851e4ba33f7723",
                                "name": "{#TECHNOLOGY}[{#RESOURCE}] USE CHANNELS",
